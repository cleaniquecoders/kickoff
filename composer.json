{
    "name": "cleaniquecoders/kickoff",
    "description": "Helps configure your new Laravel project with good practices.",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/filesystem": "^12.0",
        "illuminate/support": "^12.0",
        "symfony/console": "^6.2|^7.0",
        "symfony/polyfill-mbstring": "^1.31",
        "symfony/process": "^6.2|^7.0"
    },
    "require-dev": {
        "driftingly/rector-laravel": "^2.0",
        "larastan/larastan": "^3.6",
        "laravel/pint": "^1.24",
        "pestphp/pest": "^4.1",
        "phpstan/phpstan": "^2.1"
    },
    "bin": [
        "bin/kickoff"
    ],
    "autoload": {
        "psr-4": {
            "CleaniqueCoders\\Kickoff\\Console\\": "src/"
        },
        "files": [
            "support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CleaniqueCoders\\Kickoff\\Console\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "lint": [
            "@php vendor/bin/pint --ansi",
            "@php vendor/bin/phpstan analyse --verbose --ansi"
        ],
        "test": [
            "@php vendor/bin/pest"
        ]
    }
}
